{
  "name": "rest-my-notes",
  "version": "1.0.0",
  "description": "My Notes App",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\""
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seeder/seed.ts"
  },
  "keywords": [
    "express",
    "prisma",
    "typescript"
  ],
  "author": "Dzaki Rozaan",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "latest",
    "bcrypt": "latest",
    "cors": "latest",
    "dotenv": "latest",
    "envalid": "latest",
    "express": "latest",
    "express-async-handler": "latest",
    "http-errors": "latest",
    "jsonwebtoken": "latest",
    "module-alias": "latest",
    "morgan": "latest"
  },
  "devDependencies": {
    "@types/bcrypt": "latest",
    "@types/cors": "latest",
    "@types/express": "latest",
    "@types/http-errors": "latest",
    "@types/jsonwebtoken": "latest",
    "@types/morgan": "latest",
    "@types/node": "latest",
    "concurrently": "latest",
    "nodemon": "latest",
    "prisma": "latest",
    "ts-node": "latest",
    "typescript": "latest"
  }
}
